  "dependencies": {
    "@types/node": "^10.12.2",
    "user-async-function": "^1.1.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "es6-promise": "^4.2.5",
    "eslint": "^5.8.0",
    "eslint-plugin-import": "^2.14.0",
